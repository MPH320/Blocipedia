<h1>All Wikis</h1>
<% if user_signed_in? %>
<%= link_to "New Wiki", new_wiki_path(@wiki), class: 'btn btn-success' %>
<% end %>
 <% @wikis.each do |wiki| %>
   <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= link_to wiki.title, wiki %>
       </h4>
     </div>
   </div>
 <% end %>